class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int count=0,n=nums.size();
        int i=0;
        while(i<n){
            if(nums[i]==0){
                count++;
                nums.erase(nums.begin()+i);
                n--;
            }
            else{
                i++;
            }
        }
        for(int i=0;i<count;i++){
            nums.push_back(0);
        }
    }
};
